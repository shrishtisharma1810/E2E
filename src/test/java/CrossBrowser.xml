<?xml version="1.0" encoding="UTF-8"?>
<!-- By default execution is sequential as parameter parallel is set to false at suite level -->
<!--  <suite parallel="false" name="Suite">-->
<!-- For parallel execution set parameter parallel to tests at suite level -->
<suite parallel="tests" name="Suite">
  <test name="ChromeTest">
  <parameter name="browser" value="Chrome"></parameter>
    <classes>
      <class name="com.mycompany.app.CrossBrowser"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="EdgeTest">
  <parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="com.mycompany.app.CrossBrowser"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="FireFoxTest">
  <parameter name="browser" value="FireFox"></parameter>
    <classes>
      <class name="com.mycompany.app.CrossBrowser"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
